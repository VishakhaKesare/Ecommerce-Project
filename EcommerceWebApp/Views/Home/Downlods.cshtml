@{
    ViewBag.Title = "Downloads";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Downloads</h1>


                </div><!-- /.col -->
                <div class="col-md-5">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index")">Dashboard</a></li>
                        <li class="breadcrumb-item active">Downloads</li>
                    </ol>

                </div><!-- /.col -->
                <div class="col">
                    <div class="float-sm-right">
                        <a href="@Url.Action("AddDownlods")" data-bs-toggle="tooltip" title="" class="btn btn-primary" data-bs-original-title="Add New" aria-label="Add New"><i class="fas fa-plus"></i></a>
                        <button type="submit" form="form-subscription-plan"
                                formaction="https://demo.opencart.com/admin/index.php?route=catalog/subscription_plan|delete&amp;user_token=4ca83d273fa34bd95748b6ad76b4193a" data-bs-toggle="tooltip" title="" onclick="return confirm('Are you sure?');" class="btn btn-danger" data-bs-original-title="Delete" aria-label="Delete">
                            <i class="fas fa-trash-alt"></i>
                        </button>
                    </div>
                </div><!-- /.col -->
            </div><!-- /.row -->
            <div class="row">

            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>

    <div class="card">
        <div class="card-header"><i class="fas fa-list"></i> Download List</div>

        <table class="table table-bordered table-hover">
            <thead>
                <tr>
                    <th>
                        <div class="custom-control custom-checkbox">
                            <input class="custom-control-input custom-control-input-danger custom-control-input-outline" type="checkbox" id="customCheckbox5">
                            <label for="customCheckbox5" class="custom-control-label"></label>
                        </div>
                    </th>

                    <th>Download Name</th>
                    <th>Date Added</th>
                    <th class="text-end">Action</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="text-center" colspan="4">No results!</td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="row">
        <div class="col-sm-6 text-start"></div>
        <div class="col-sm-6 text-end">Showing 0 to 0 of 0 (0 Pages)</div>
    </div>
</div>
</div>
