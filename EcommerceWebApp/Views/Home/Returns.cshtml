
@{
    ViewBag.Title = "Returns";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">product Returns</h1>


                </div><!-- /.col -->
                <div class="col-md-5">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index")" ">Dashboard</a></li>
                        <li class="breadcrumb-item active">Product Returns</li>
                    </ol>

                </div><!-- /.col -->
                <div class="col">
                    <div class="float-sm-right">
                        <a href="@Url.Action("AddProductReturns")" data-bs-toggle="tooltip" title="" class="btn btn-primary" data-bs-original-title="Add New" aria-label="Add New"><i class="fas fa-plus"></i></a>
                        <button type="submit" form="form-subscription-plan"
                                formaction="https://demo.opencart.com/admin/index.php?route=catalog/subscription_plan|delete&amp;user_token=4ca83d273fa34bd95748b6ad76b4193a" data-bs-toggle="tooltip" title="" onclick="return confirm('Are you sure?');" class="btn btn-danger" data-bs-original-title="Delete" aria-label="Delete">
                            <i class="fas fa-trash-alt"></i>
                        </button>
                    </div>
                </div><!-- /.col -->
            </div><!-- /.row -->
            <div class="row">

            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- Main content -->

    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-9">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title"><i class="fas fa-list"></i> &nbsp;product Returns Lists</h3>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="productReturnsTable">
                                    <thead>
                                        <tr>
                                            <th>
                                                <input type="checkbox" id="exampleCheck1">
                                            </th>
                                            <th>Return Id</th>
                                            <th>Ordered Id</th>
                                            <th>Customer </th>

                                            <th>product</th>
                                            <th>Model</th>
                                            <th>status</th>
                                            <th>Date Added</th>
                                            <th>Date Modified</th>
                                            <th style="width: 40px">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title"><i class="fas fa-filter"></i> Filter</h3>
                        </div>
                        <div class="card-body">
                            <label>Return ID</label>
                            <input class="form-control form-control-lg" type="text" placeholder="Return Id">
                            <br>
                            <label>Order Id</label>
                            <input class="form-control" type="text" placeholder="Order ID">
                            <br>
                            <label>Customer</label>
                            <input class="form-control" type="text" placeholder="Customer">
                            <br>
                            <label>product</label>
                            <input class="form-control" type="text" placeholder="product">
                            <br>
                            <label>Model</label>
                            <input class="form-control" type="text" placeholder="Model">
                            <br>
                            <div class="form-group">
                                <label>Return Status</label>
                                <select class="form-control">
                                    <option>Default</option>

                                </select>
                            </div>
                            <br>


                            <div class="form-group">
                                <label>Date Added:</label>
                                <div class="input-group date" id="reservationdate" data-target-input="nearest">
                                    <input type="text" class="form-control datetimepicker-input" data-target="#reservationdate">
                                    <div class="input-group-append" data-target="#reservationdate" data-toggle="datepicker">
                                        <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label>Date Modified:</label>
                            <div class="input-group date" id="reservationdate" data-target-input="nearest">
                                <input type="text" class="form-control datetimepicker-input" data-target="#reservationdate">
                                <div class="input-group-append" data-target="#reservationdate" data-toggle="datepicker">
                                    <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                                </div>
                            </div>
                        </div>
                    </div>



                </div>





    </section>
</div>
